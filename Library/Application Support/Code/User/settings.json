{
    "explorer.openEditors.visible": 0,
    "C_Cpp.intelliSenseEngine": "Default",
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabSizing": "shrink",
    "window.zoomLevel": -1.2,
    "editor.wordWrap": "on",
    "editor.fontSize": 17,
    "workbench.editor.showTabs": false,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;'\",.<>/?",
    "editor.lineNumbers": "off",
    "editor.renderLineHighlight": "none",
    "explorer.autoReveal": true,
    "editor.minimap.enabled": false,
    "editor.folding": false,
    "editor.glyphMargin": false,
    "git.decorations.enabled": false,
    "editor.tabCompletion": "on",
    "workbench.activityBar.visible": false,
    "editor.lineDecorationsWidth": 0,
    "editor.scrollbar.useShadows": false,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.roundedSelection" : false,
    "editor.renderIndentGuides": false,
    "[rust]": {
        "editor.formatOnSave": true
    },
    "vim.enableNeovim": false,
    "vim.hlsearch": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.normalModeKeyBindings": [
        {
            "before": [ "w" ],
            "after": [ "<C-w>" ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "w", "s" ],
            "commands": [":sp"]
        },
        {
            "before": [ "[", "o", "n" ],
            "commands": [":set number"]
        },
        {
            "before": [ "]", "o", "n" ],
            "commands": [":set number!"]
        },
        {
            "before": [ "[","[" ], "after": ["z", "t"],
            "commands": [ { "command": "gotoNextPreviousMember.previousMember", "args": [] } ],
        },
        {
            "before": ["<esc>"],
            "commands": [":nohl"]
        },
        {
            "before": [ "]","]" ], "after": ["z", "t"],
            "commands": [ { "command": "gotoNextPreviousMember.nextMember", "args": [] } ],
        },
        {
            "before": ["Q"], "after": [ ],
            "commands": [ { "command": "workbench.action.closeActiveEditor", "args": [] } ]
        },
        {
            "before": ["W"], "after": [ ],
            "commands": [ { "command": "workbench.action.files.save", "args": [] } ]
        },
        {
            "before": ["K"], "after": [ ],
            "commands": [ { "command": "editor.action.showHover", "args": [] } ]
        }
    ],

    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Default Light+",
    "rust.rustup": {
        "toolchain": "nightly-x86_64-apple-darwin",
        "nightlyToolchain": "nightly-x86_64-apple-darwin"
    },
    "rust.mode": "rls",
    "rust.rls": {
        "useRustfmt": true
    },
}