{
    "explorer.openEditors.visible": 0,
    "C_Cpp.intelliSenseEngine": "Default",
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabSizing": "shrink",
    "window.zoomLevel": -1.2,
    "editor.wordWrap": "on",
    "editor.fontSize": 17,
    "workbench.editor.showTabs": false,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;'\",.<>/?",
    "editor.lineNumbers": "off",
    "editor.renderLineHighlight": "none",
    "editor.minimap.enabled": false,
    "editor.folding": false,
    "editor.glyphMargin": false,
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.renderIndentGuides": false,
    },
    "editor.tabCompletion": "on",
    "editor.renderIndentGuides": false,
    "rust-client.channel": "nightly",
    "[rust]": {
        "editor.formatOnSave": true
    },
    "vim.enableNeovim": false,
    "vim.hlsearch": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.normalModeKeyBindings": [
        {
            "before": [ "w" ],
            "after": [ "<C-w>" ]
        },

    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "[","[" ], "after": ["z", "t"],
            "commands": [ { "command": "gotoNextPreviousMember.previousMember", "args": [] } ],
        },
        {
            "before": ["<esc>"],
            "commands": [":nohl"]
        },
        {
            "before": [ "]","]" ], "after": ["z", "t"],
            "commands": [ { "command": "gotoNextPreviousMember.nextMember", "args": [] } ],
        },
        {
            "before": ["Q"], "after": [ ],
            "commands": [ { "command": "workbench.action.closeActiveEditor", "args": [] } ]
        },
        {
            "before": ["W"], "after": [ ],
            "commands": [ { "command": "workbench.action.files.save", "args": [] } ]
        },
        {
            "before": ["K"], "after": [ ],
            "commands": [ { "command": "editor.action.showHover", "args": [] } ]
        }
    ],

    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Default Light+",
    "cmake-tools-helper.cmake_download_path": "/Users/amin/.vscode/extensions/maddouri.cmake-tools-helper-0.2.1/cmake_download",
    "cmake.configureOnOpen": true,
    "rust.rustup": {
        "toolchain": "nightly-x86_64-apple-darwin",
        "nightlyToolchain": "nightly-x86_64-apple-darwin"
    },
    "rust.mode": "rls",
    "rust.rls": {
        "useRustfmt": true
    },
}
